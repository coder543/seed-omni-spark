# Track-B (Omni) only

# S3/OBJECT STORAGE (required for image/audio generation)
# Local MinIO defaults (see docker-compose.track-b.yaml)
NCP_S3_SERVICE=s3
NCP_S3_ENDPOINT=http://minio:9000
NCP_S3_REGION=us-east-1
NCP_S3_ACCESS_KEY=minio
NCP_S3_SECRET_KEY=minio123
NCP_S3_BUCKET_NAME=omni
WBL_S3_BUCKET_NAME=${NCP_S3_BUCKET_NAME}
S3_FORCE_PATH_STYLE=1

# MinIO root credentials (for local S3 in compose)
MINIO_ROOT_USER=minio
MINIO_ROOT_PASSWORD=minio123

# Model IDs
OMNI_ENCODER_VISION_MODEL_ID=track_b_vision_encoder
OMNI_ENCODER_AUDIO_MODEL_ID=track_b_audio_encoder

# Track-B model paths (after conversion)
OMNI_MODEL_PATH=/path/to/models/track_b/llm/HyperCLOVAX-SEED-Omni-8B
OMNI_ENCODER_AUDIO_MODEL_PATH=/path/to/models/track_b/ae/HyperCLOVAX-SEED-Omni-8B
OMNI_ENCODER_VISION_MODEL_PATH=/path/to/models/track_b/ve/HyperCLOVAX-SEED-Omni-8B
OMNI_DECODER_VISION_MODEL_PATH=/path/to/models/track_b/vd/HyperCLOVAX-SEED-Omni-8B
OMNI_DECODER_AUDIO_TORCHSERVE_MODEL_PATH=/path/to/models/track_b/ad/HyperCLOVAX-SEED-Omni-8B

# Ports (host)
OMNI_CHAINER_API_PORT=8000
OMNI_PORT=10032
OMNI_ENCODER_AUDIO_API_PORT=10002
OMNI_ENCODER_VISION_API_PORT=10064
OMNI_DECODER_VISION_API_PORT=10063
OMNI_DECODER_AUDIO_API_PORT=11180
OMNI_DECODER_AUDIO_TORCHSERVE_PORT=11181

# vLLM runtime tuning (optional)
# Example: OMNI_VLLM_EXTRA_ARGS="--enforce-eager"
# Example: OMNI_VLLM_EXTRA_ARGS="--cudagraph-capture-sizes 1 2 4 8"
OMNI_VLLM_GPU_MEMORY_UTILIZATION=0.35
OMNI_VLLM_EXTRA_ARGS=--enforce-eager

# Web UI bind host/port
WEBUI_HOST=0.0.0.0
WEBUI_PORT=3000

# Optional: write decoded audio chunks for debugging (path inside webui container).
# Example: AUDIO_DEBUG_DIR=/app/audio-debug
AUDIO_DEBUG_DIR=

# Audio decoder / torchserve tuning
AUDIO_DECODER_SAMPLE_RATE=24000
AUDIO_TORCHSERVE_FORMAT=pcm
AUDIO_PCM_SAMPLE_RATE=24000
