diff --git a/docker/vllm.aarch64.dockerfile b/docker/vllm.aarch64.dockerfile
new file mode 100644
index 0000000..78a52b9
--- /dev/null
+++ b/docker/vllm.aarch64.dockerfile
@@ -0,0 +1,46 @@
+# AArch64 build of vLLM for OmniServe
+FROM nvcr.io/nvidia/pytorch:25.10-py3
+
+ENV DEBIAN_FRONTEND=noninteractive \
+    PYTHONUNBUFFERED=1 \
+    PYTHONDONTWRITEBYTECODE=1
+
+RUN apt-get update && apt-get install -y --no-install-recommends \
+    git curl wget patch \
+    build-essential cmake ninja-build \
+    && rm -rf /var/lib/apt/lists/*
+
+# vLLM build setup
+ENV TORCH_CUDA_ARCH_LIST=12.0
+ENV TRITON_PTXAS_PATH=/usr/local/cuda/bin/ptxas
+
+WORKDIR /vllm-workspace
+
+# Storage utility
+COPY util/storage/wbl_storage_utility /wbl_storage_utility
+RUN pip install --no-cache-dir /wbl_storage_utility
+
+# vLLM plugins
+COPY vllm-plugins /vllm-workspace/vllm-plugins
+RUN pip install --no-cache-dir -r /vllm-workspace/vllm-plugins/requirements.txt && \
+    pip install -e /vllm-workspace/vllm-plugins
+
+# vLLM source (OmniServe fork)
+COPY vllm /vllm-workspace/vllm
+
+WORKDIR /vllm-workspace/vllm
+
+RUN python3 use_existing_torch.py
+RUN pip install --upgrade pip
+RUN pip install --no-cache-dir -r requirements/build.txt
+RUN pip install --no-cache-dir -r requirements/cuda.txt
+RUN pip install --no-cache-dir torchao==0.14.1
+
+# Build/install vLLM (non-editable to avoid setuptools editable issues)
+ENV SETUPTOOLS_SCM_PRETEND_VERSION_FOR_VLLM=0.11.2
+ENV SETUPTOOLS_SCM_PRETEND_VERSION=0.11.2
+RUN pip install --no-build-isolation . -v
+
+WORKDIR /
+
+EXPOSE 10032
